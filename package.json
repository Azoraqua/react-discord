  "commitAnalyizer": {
    "preset": "conventionalcommits",
    "releaseRules": [
      {
        "type": "chore",
        "scope": "deps",
        "release": "patch"
      },
      {
        "type": "fix",
        "release": "patch"
      },
      {
        "type": "feat",
        "release": "minor"
      },
      {
        "type": "BREAKING CHANGE",
        "release": "major"
      },
      {
        "type": "docs",
        "release": "patch"
      },
      {
        "type": "style",
        "release": "patch"
      },
      {
        "type": "refactor",
        "release": "patch"
      },
      {
        "type": "perf",
        "release": "patch"
      },
      {
        "type": "test",
        "release": "patch"
      }
    ]
  }
}
